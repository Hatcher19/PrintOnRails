<%=
form.has_many :addresses, :wrapper_html=> { :class => 'panel' } do |address|  
  address.input :address_type, :collection => Address::ADD
  address.input :address_name, label: "Attn"
  address.input :street 
  address.input :city 
  address.input :state
  address.input :zip
  address.input :same_as_billing, label: "Shipping same as billing?"
end 
%>