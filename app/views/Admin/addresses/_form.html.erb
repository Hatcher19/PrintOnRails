<%=
form.has_many :addresses do |address| 
address.inputs "Address" do
    address.inputs :class => "" do 
      address.input :address_name, label: "Address Type", hint: "IE: Billing, Shipping, X Franchise, Y Franchise, etc."
      address.input :street 
      address.input :city 
      address.input :state
      address.input :zip
    end 
  end 
end 
%>