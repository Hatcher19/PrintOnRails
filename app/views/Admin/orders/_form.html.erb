<%=
semantic_form_for @order, :builder => ActiveAdmin::FormBuilder do |f|
	f.input "Order Information" do
		f.input :customer_id
		f.input :order_type_id
		f.input :order_status_id
		f.input :order_category_id
		f.input :start_date
		f.input :end_date
		f.input :name 
		f.input :color_front
		f.input :color_back
		f.input :color_sleeve
		f.input :whiteboard
	end
end
%>